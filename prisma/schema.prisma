generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model Profile {
    id String @id @default(uuid())

    email    String @db.Text
    name     String
    imageUrl String @db.Text

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
